<%= f.fields_for :newsroom, @newsroom do |newsroom| %>

<div class="question" id="q0">
    <div class="container-fluid">
        <div class="row">
            <div class="col-xs-12">
                <h4 class="question-title">Question 1</h4>
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <i class="fa fa-close pull-right"></i>
                        <i class="fa fa-question-circle fa-lg intHelp pull-right hidden-xs text-primary hints" id="1" onClick="reply_click(this.id)" data-toggle="tooltip" data-placement="left" title="Toggle help"></i>
                        <p class="panel-title text-left">Company Name</p>
                    </div> <!-- / .panel-heading -->
                    <div class="panel-body">
                        <%= newsroom.text_field :company_name, class: "form-control fixed-textarea", rows: "4", id: "textarea", onfocus: "this.value = this.value;" %>
                    </div> <!-- / .panel-body -->
                    <div class="panel-footer clearfix">
                        <div class="pull-left" id="textareaFeedback"></div>
                        <!-- <button type="button" class="btn btn-primary pull-right" id="save">Save <i class="fa fa-arrow-right"></i></button> -->
                    </div><!--/ .panel-footer -->
                </div> <!-- / .panel -->
            </div> <!-- / .col-xs-12 -->
        </div> <!-- / .row -->
    </div> <!-- / .container-fluid -->
</div> <!-- / .question -->

<div class="question" id="team_members">
    <div class="container-fluid">
        <div class="row">
            <div class="col-xs-12">
                <h4 class="question-title">Question 2</h4>
								<%= newsroom.fields_for :people do |person| %>
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <%= person.link_to_remove do %><i class="fa fa-close pull-right"></i><% end %>
                        <i class="fa fa-question-circle fa-lg intHelp pull-right hidden-xs text-primary hints" id="2" onClick="reply_click(this.id)"  data-toggle="tooltip" data-placement="left" title="Toggle help"></i>
                        <p class="panel-title text-left">Team Members</p>
                    </div> <!-- / .panel-heading -->
                    <div class="panel-body">
                        
												<%= person.text_field :name, class: "form-control", placeholder: "Name" %><br>
												Founder <%= person.check_box :founder %>
												<%= person.text_field :role, class: "form-control", placeholder: "Role"  %><br>
												<%= person.text_area :presentation, class: "form-control", placeholder: "Presentation" %><br>
												
                    </div> <!-- / .panel-body -->
                    <div class="panel-footer clearfix">
                        <div class="pull-left" id="textareaFeedback"></div>
                        <!-- <button type="button" class="btn btn-primary pull-right" id="save">Save <i class="fa fa-arrow-right"></i></button> -->
                    </div><!--/ .panel-footer -->
                </div> <!-- / .panel -->
								<% end  %>
								<p class="text-center"><%= newsroom.link_to_add :people, class: "add-new" do %> + Add person<% end %></p>
            </div> <!-- / .col-xs-12 -->
        </div> <!-- / .row -->
    </div> <!-- / .container-fluid -->
</div> <!-- / .question -->

