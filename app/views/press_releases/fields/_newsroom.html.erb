<div class="question" id="funding">
    <div class="container-fluid">
        <div class="row">
            <div class="col-xs-12">
                <h4 class="question-title">Question 9</h4>
								<%= newsroom.fields_for :fundings do |funding| %>
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <%= funding.link_to_remove do %><i class="fa fa-close pull-right"></i><% end %>
                        <%= link_to "#" do %><i class="fa fa-question-circle fa-lg intHelp pull-right hidden-xs text-primary hints" data-toggle="tooltip" data-placement="left" title="Toggle help"></i><% end %>
                        <p class="panel-title text-left">Funding</p>
                    </div> <!-- / .panel-heading -->
                    <div class="panel-body">
                        
												<%= funding.select :investment_type, ["Private Investor", "Venture Capital", "Bootstrapped", "No funding", "Other"], class: "form-control", id: "exampleInputEmail1", placeholder: "Name"  %>
												<%= funding.text_field :name, class: "form-control", id: "exampleInputEmail1", placeholder: "Name"  %></strong> <%= funding.date_select :date, class: "form-control"  %>
																																$
												<%= funding.number_field :amount, class: "form-control", placeholder: "Amount"  %>
												
                    </div> <!-- / .panel-body -->
                    <div class="panel-footer clearfix">
                        <div class="pull-left" id="textareaFeedback"></div>
                        <!-- <button type="button" class="btn btn-primary pull-right" id="save">Save <i class="fa fa-arrow-right"></i></button> -->
                    </div><!--/ .panel-footer -->
                </div> <!-- / .panel -->
								<p class="text-center"><%= newsroom.link_to_add :fundings, class: "add-new" do %> + Add funding<% end %></p>
								<% end  %>
            </div> <!-- / .col-xs-12 -->
        </div> <!-- / .row -->
    </div> <!-- / .container-fluid -->
